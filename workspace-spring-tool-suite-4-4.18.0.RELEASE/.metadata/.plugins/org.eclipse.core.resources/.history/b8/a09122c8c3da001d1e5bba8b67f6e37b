package first;

class Bhopal
{
	int truck;
	int car;
	String health;
	public Bhopal() {
		
	}
	public Bhopal(int truck, int car, String health) {
		this.truck = truck;
		this.car = car;
		this.health = health;
	}
	
	
}



class Sumans extends Bhopal {

	private int age;
	private String family;
	private int workHours;
	public Sumans(int age, String family, int workHours) {
		super();
		this.age = age;
		this.family = family;
		this.workHours = workHours;
	}
	
	 
	 
	 
}
