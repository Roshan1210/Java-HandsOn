package first;

class Bhopal
{
	int truck;
	int car;
	String health;
	public Bhopal() {
		
	}
	public Bhopal(int truck, int car, String health) {
		this.truck = truck;
		this.car = car;
		this.health = health;
	}
	
	
}



class Sumans extends Bhopal {

	private int age;
	private String family;
	private int workHours;
	public Sumans(int age, String family, int workHours) {
		super();
		this.age = age;
		this.family = family;
		this.workHours = workHours;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getFamily() {
		return family;
	}
	public void setFamily(String family) {
		this.family = family;
	}
	public int getWorkHours() {
		return workHours;
	}
	public void setWorkHours(int workHours) {
		this.workHours = workHours;
	}
	
	private void enjoy () {
		System.out.println("In enjoy method");
	}
	String more() {
		enjoy();
		return "Enjoy more";
	}
	
	void method(int s,String y,char z) {
		System.out.println("In enjoy method"+s+" "+y+" "+z);
		
	}
	
	 
	 
	 
}
